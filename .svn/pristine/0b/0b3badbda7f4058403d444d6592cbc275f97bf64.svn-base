<ion-header header>
</ion-header>
<ion-content>
    <div id="app_form_page" class="mainContainer">
        <title-page sub1="แบบแสดงข้อมูลผู้ถือหน่วยลงทุน">ใบคำขอ</title-page>
        <div class="boxContentPage bgBlue1">
            <div class="container xs-pdlr0">
                <form [formGroup]="appInfo">
                    <div class="boxListForm pd0 pdt15">
                        <div class="col-lg-6 col-md-6 pd0">
                            <div class="col-lg-4 col-md-4 col-sm-3 txt-sm-right pdr-sm-7 lh-form-control">
                                ผู้ขอเอาประกันภัย :
                            </div>
                            <div class="col-lg-8 col-md-8 col-sm-9 pdl-sm-7">
                                <div class="form-group">
                                    <input id="customername" formControlName="customername" class="form-control" type="text" />
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-6 pd0">
                            <div class="col-lg-5 col-md-5 col-sm-6 txt-sm-right pdr-sm-7 lh-form-control">
                                เพศ :
                            </div>
                            <div class="col-lg-7 col-md-7 col-sm-6 pdl-sm-7">
                                <div class="form-group">
                                    <input id="gender" formControlName="gender" class="form-control" type="text" />
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-6 pd0">
                            <div class="col-lg-5 col-md-5 col-sm-6 txt-sm-right pdr-sm-7 lh-form-control">
                                อายุ :
                            </div>
                            <div class="col-lg-7 col-md-7 col-sm-6 pdl-sm-7">
                                <div class="form-group">
                                    <input id="age" formControlName="age" class="form-control" type="text" />
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6 col-md-6 pd0">
                            <div class="col-lg-4 col-md-4 col-sm-3 txt-sm-right pdr-sm-7 lh-form-control">
                                แบบประกัน :
                            </div>
                            <div class="col-lg-8 col-md-8 col-sm-9 pdl-sm-7">
                                <div class="form-group">
                                    <input id="planname" formControlName="planname" class="form-control" type="text" />
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-6 pd0">
                            <div class="col-lg-5 col-md-5 col-sm-6 txt-sm-right pdr-sm-7 lh-form-control">
                                ชำระเบี้ย :
                            </div>
                            <div class="col-lg-7 col-md-7 col-sm-6 pdl-sm-7">
                                <div class="form-group">
                                    <input id="payment" formControlName="payment" class="form-control" type="text" />
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-6 pd0">
                            <div class="col-lg-5 col-md-5 col-sm-6 txt-sm-right pdr-sm-7 lh-form-control">
                                ชั้นอาชีพ :
                            </div>
                            <div class="col-lg-7 col-md-7 col-sm-6 pdl-sm-7">
                                <div class="form-group">
                                    <input id="occupationtype" formControlName="occupationtype" class="form-control" type="text" />
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>

        <div class="boxContentPage">
            <steps design="2" (changeIndex)="changeIndex($event)" [selectIndex]="selectedStep">

                <!-- STEP 1 -->
                <step index="0" stepTitle="ข้อมูลทั่วไป">
                    <!-- <form [formGroup]="appGeneral"> -->
                    <div class="boxContentPage bgBlue3 hasBgImg bgImg95">
                        <div class="container">
                            <div class="bgPaperShadow mgt25">
                                <div class="contentPaperShadow">
                                    <form [formGroup]="appGeneral">
                                        <!-- start boxFixTopJumpMenu -->
                                        <div class="boxFixTopJumpMenu">
                                            <div id="boxFixJumpMenu" class="boxContentFixJumpMenu">
                                                <div class="boxListFixJumpMenu">
                                                    <a class="action">ข้อมูลทั่วไป ผู้ขอเอาประกัน/ผู้เอาประกัน</a>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- end boxFixTopJumpMenu -->
                                        <div id="boxListJumpContent" class="boxListJumpContent">
                                            <!-- start oneJumpContent -->
                                            <div class="oneJumpContent">
                                                <div class="pdt20">
                                                    <div class="boxListForm pdlr0 pdb10 xs-pdt0">
                                                        <div class="col-lg-4 col-md-6 pd0">
                                                            <div class="col-lg-5 col-md-4 col-sm-6 txt-sm-right pdr-sm-7 lh-form-control">
                                                                ข้าพเจ้า :
                                                            </div>
                                                            <div class="col-lg-7 col-md-8 col-sm-6 pdl-sm-7">
                                                                <div class="form-group">
                                                                    <input class="form-control" id="username" formControlName="username" type="text" />
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-lg-4 col-md-6 pd0">
                                                            <div class="col-lg-5 col-md-4 col-sm-6 txt-sm-right pdr-sm-7 lh-form-control">
                                                                เพศ :
                                                            </div>
                                                            <div class="col-lg-7 col-md-8 col-sm-6 pdl-sm-7">
                                                                <dropdown formControlName="gender">
                                                                    <dropdown-option value="M">ชาย</dropdown-option>
                                                                    <dropdown-option value="F">หญิง</dropdown-option>
                                                                </dropdown>
                                                            </div>
                                                        </div>
                                                        <div class="col-lg-4 col-md-6 pd0">
                                                            <div class="col-lg-5 col-md-4 col-sm-6 txt-sm-right pdr-sm-7 lh-form-control">
                                                                วันเกิด :
                                                            </div>
                                                            <div class="col-lg-7 col-md-8 col-sm-6 pdl-sm-7">
                                                                <!-- <calendar formControlName="birthdate" [disabled]="true"></calendar> -->
                                                                <div class="form-group">
                                                                    <input class="form-control" type="text" formControlName="birthdate" disabled/>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-lg-4 col-md-6 pd0">
                                                            <div class="col-lg-5 col-md-4 col-sm-6 txt-sm-right pdr-sm-7 lh-form-control">สัญชาติ :</div>
                                                            <div class="col-lg-7 col-md-8 col-sm-6 pdl-sm-7">
                                                                <dropdown formControlName="nationality">
                                                                    <dropdown-option [value]="item.key1" *ngFor="let item of countryDropdown">{{ item.th }}</dropdown-option>
                                                                </dropdown>
                                                            </div>
                                                        </div>
                                                        <div *ngIf="appGeneral.get('nationality').value != 'THA'" class="col-lg-offset-1 col-lg-7 pd0">

                                                            <div class="col-md-8 col-sm-6 txt-sm-right pdr-sm-7 lh-form-control">หมายเลขประจำตัวผู้เสียภาษี (ชาวต่างชาติ) :</div>
                                                            <div class="col-md-4 col-sm-6 pdl-sm-7">
                                                                <div class="form-group">
                                                                    <input class="form-control" formControlName="taxid" type="text" maxlength="13" />
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <hr />

                                                <div class="pdt10 pdb10 xs-pdlr15 lh-form-control">
                                                    เอกสารประกอบ ได้แก่
                                                </div>
                                                <div class="col-md-12 pd0">
                                                    <div class="col-sm-offset-1 col-lg-3 col-sm-11 xs-pdlr15 pd0">
                                                        <div class="form-group">
                                                            <input id="doc01" class="check-style3" type="checkbox" formControlName="citizenCheck" />
                                                            <label for="doc01">สำเนาบัตรประจำตัวประชาชน</label>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-4 col-md-6 pd0">
                                                        <div class="col-lg-5 col-md-4 col-sm-6 txt-sm-right pdr-sm-7 lh-form-control">เลขที่ :</div>
                                                        <div class="col-lg-7 col-md-8 col-sm-6 pdl-sm-7">
                                                            <div class="form-group">
                                                                <input class="form-control" formControlName="citizenid" type="text" maxlength="13" />
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-4 col-md-6 pd0">
                                                        <div class="col-lg-5 col-md-4 col-sm-6 txt-sm-right pdr-sm-7 lh-form-control">วันหมดอายุบัตร :</div>
                                                        <div class="col-lg-7 col-md-8 col-sm-6 pdl-sm-7">
                                                            <div class="form-group">
                                                                <input class="form-control" type="text" formControlName="expiredate" />
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="clearfix"></div>

                                                <div class="col-md-12 pd0">
                                                    <div class="col-sm-offset-1 col-lg-3 col-sm-11 xs-pdlr15 pd0">
                                                        <div class="form-group">
                                                            <input id="doc02" formControlName="houseParticularsCheck" class="check-style3" type="checkbox" />
                                                            <label for="doc02">สำเนาทะเบียนบ้าน</label>
                                                        </div>
                                                    </div>
                                                </div>


                                                <div class="col-md-12 pd0">
                                                    <div class="col-sm-offset-1 col-lg-3 col-sm-11 xs-pdlr15 pd0">
                                                        <div class="form-group">
                                                            <input id="doc03" class="check-style3" formControlName="passportCheck" type="checkbox" />
                                                            <label for="doc03">สำเนาหนังสือเดินทาง (Passport)</label>
                                                        </div>
                                                    </div>
                                                    <div *ngIf="(appGeneral.get('passportCheck').value)" class="col-lg-4 col-md-6 pd0">
                                                        <div class="col-lg-5 col-md-4 col-sm-6 txt-sm-right pdr-sm-7 lh-form-control">เลขที่ :</div>
                                                        <div class="col-lg-7 col-md-8 col-sm-6 pdl-sm-7">
                                                            <div class="form-group">
                                                                <input class="form-control" formControlName="passportValue" type="text" maxlength="13" />
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="col-md-12 pd0">
                                                    <div class="col-sm-offset-1 col-sm-11 pd0">
                                                        <div class="inline_block pdr-sm-7 xs-pdlr15 doc04-label">
                                                            <div class="form-group">
                                                                <input id="doc04" class="check-style3" formControlName="otherDescCheck" type="checkbox" />
                                                                <label for="doc04">อื่นๆ (โปรดระบุ)</label>
                                                            </div>
                                                        </div>
                                                        <div *ngIf="(appGeneral.get('otherDescCheck').value)" class="inline_block pdl-sm-7 xs-pdlr15 doc04-input">
                                                            <div class="form-group">
                                                                <input class="form-control" formControlName="otherDescValue" type="text" />
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <hr />
                                            </div>
                                            <!-- end oneJumpContent -->


                                            <!-- start oneJumpContent -->
                                            <div class="oneJumpContent">
                                                <!-- start Adress Tab -->
                                                <div id="boxListContentTabStyle3" class="boxListContentTabStyle3">
                                                    <tabs>
                                                        <tab tabTitle="ที่อยู่ปัจจุบัน">
                                                            <div class="oneContentTab">
                                                                <div class="boxListForm pdlr0" style="overflow-y: auto;">
                                                                    <div class="col-lg-4 col-md-6">
                                                                        <div class="row">
                                                                            <div class="col-lg-5 col-md-4 col-sm-6 col-xs-12 txt-sm-right pdr-sm-7 lh-form-control ws-nowrap">
                                                                                <font color="red">*</font> เลขที่ :
                                                                            </div>
                                                                            <div class="col-lg-7 col-md-8 col-sm-6 col-xs-12 pdl-sm-7">
                                                                                <div class="form-group">
                                                                                    <input class="form-control" maxlength="10" type="text" formControlName="permanentAddressNo" NumberCharacterOnly />
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    <div class="col-lg-4 col-md-6">
                                                                        <div class="row">
                                                                            <div class="col-lg-5 col-md-4 col-sm-6 col-xs-12 txt-sm-right pdr-sm-7 lh-form-control ws-nowrap">หมู่บ้าน/อาคาร :</div>
                                                                            <div class="col-lg-7 col-md-8 col-sm-6 col-xs-12 pdl-sm-7">
                                                                                <div class="form-group">
                                                                                    <input class="form-control" maxlength="32" formControlName="permanentVillage" type="text" NumberCharacterOnly/>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    <div class="col-lg-4 col-md-6">
                                                                        <div class="row">
                                                                            <div class="col-lg-5 col-md-4 col-sm-6 col-xs-12 txt-sm-right pdr-sm-7 lh-form-control ws-nowrap">หมู่ที่ :</div>
                                                                            <div class="col-lg-7 col-md-8 col-sm-6 col-xs-12 pdl-sm-7">
                                                                                <div class="form-group">
                                                                                    <input class="form-control" maxlength="2" formControlName="permanentMoo" type="text" NumberCharacterOnly/>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    <div class="col-lg-4 col-md-6">
                                                                        <div class="row">
                                                                            <div class="col-lg-5 col-md-4 col-sm-6 col-xs-12 txt-sm-right pdr-sm-7 lh-form-control ws-nowrap">ตรอก/ซอย :</div>
                                                                            <div class="col-lg-7 col-md-8 col-sm-6 col-xs-12 pdl-sm-7">
                                                                                <div class="form-group">
                                                                                    <input class="form-control" maxlength="20" formControlName="permanentSoi" type="text" NumberCharacterOnly />
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    <div class="col-lg-4 col-md-6">
                                                                        <div class="row">
                                                                            <div class="col-lg-5 col-md-4 col-sm-6 col-xs-12 txt-sm-right pdr-sm-7 lh-form-control ws-nowrap">ถนน :</div>
                                                                            <div class="col-lg-7 col-md-8 col-sm-6 col-xs-12 pdl-sm-7">
                                                                                <div class="form-group">
                                                                                    <input class="form-control" maxlength="20" formControlName="permanentRoad" type="text" NumberCharacterOnly/>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    <div class="col-lg-4 col-md-6">
                                                                        <div class="row">
                                                                            <div class="col-lg-5 col-md-4 col-sm-6 col-xs-12 txt-sm-right pdr-sm-7 lh-form-control ws-nowrap">
                                                                                <font color="red">*</font> ตำบล/แขวง :
                                                                            </div>
                                                                            <div class="col-lg-7 col-md-8 col-sm-6 col-xs-12 pdl-sm-7">
                                                                                <div class="form-group hasSelect">
                                                                                    <address addressType="tambon_name" formControlName="subdistrict" [(address)]="appGeneral.value.permanentAddress.addressSelected"></address>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    <div class="col-lg-4 col-md-6">
                                                                        <div class="row">
                                                                            <div class="col-lg-5 col-md-4 col-sm-6 col-xs-12 txt-sm-right pdr-sm-7 lh-form-control ws-nowrap">
                                                                                <font color="red">*</font> อำเภอ/เขต :
                                                                            </div>
                                                                            <div class="col-lg-7 col-md-8 col-sm-6 col-xs-12 pdl-sm-7">
                                                                                <div class="form-group hasSelect">
                                                                                    <address addressType="amphur_name" formControlName="district" [(address)]="appGeneral.value.permanentAddress.addressSelected"></address>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    <div class="col-lg-4 col-md-6">
                                                                        <div class="row">
                                                                            <div class="col-lg-5 col-md-4 col-sm-6 col-xs-12 txt-sm-right pdr-sm-7 lh-form-control ws-nowrap">
                                                                                <font color="red">*</font> จังหวัด :</div>
                                                                            <div class="col-lg-7 col-md-8 col-sm-6 col-xs-12 pdl-sm-7">
                                                                                <div class="form-group hasSelect">
                                                                                    <address addressType="province_name" formControlName="province" [(address)]="appGeneral.value.permanentAddress.addressSelected"></address>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    <div class="col-lg-4 col-md-6">
                                                                        <div class="row">
                                                                            <div class="col-lg-5 col-md-4 col-sm-6 col-xs-12 txt-sm-right pdr-sm-7 lh-form-control ws-nowrap">
                                                                                <font color="red">*</font> รหัสไปรษณีย์ :
                                                                            </div>
                                                                            <div class="col-lg-7 col-md-8 col-sm-6 col-xs-12 pdl-sm-7">
                                                                                <div class="form-group hasSelect">
                                                                                    <address addressType="zip" formControlName="postcode" [(address)]="appGeneral.value.permanentAddress.addressSelected"></address>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    <div class="col-lg-4 col-md-6">
                                                                        <div class="row">
                                                                            <div class="col-lg-5 col-md-4 col-sm-6 col-xs-12 txt-sm-right pdr-sm-7 lh-form-control ws-nowrap">
                                                                                <font color="red">*</font> ประเทศ :</div>
                                                                            <div class="col-lg-7 col-md-8 col-sm-6 col-xs-12 pdl-sm-7">
                                                                                <dropdown id="permanentCountry" formControlName="permanentCountry">
                                                                                    <dropdown-option [value]="item.key2" *ngFor="let item of countryDropdown">{{ item.th }}</dropdown-option>
                                                                                </dropdown>
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    <div class="col-lg-4 col-md-6">
                                                                        <div class="row">
                                                                            <div class="col-lg-5 col-md-4 col-sm-6 col-xs-12 txt-sm-right pdr-sm-7 lh-form-control ws-nowrap">โทรศัพท์บ้าน :</div>
                                                                            <div class="col-lg-7 col-md-8 col-sm-6 col-xs-12 pdl-sm-7">
                                                                                <div class="form-group">
                                                                                    <input name="tel_no" formControlName="permanentTelNo" maxlength="10" class="form-control" type="text" NumberOnly />
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    <div class="col-lg-4 col-md-6">
                                                                        <div class="row">
                                                                            <div class="col-lg-5 col-md-4 col-sm-6 col-xs-12 txt-sm-right pdr-sm-7 lh-form-control ws-nowrap">
                                                                                <font color="red">*</font> โทรศัพท์มือถือ :
                                                                            </div>
                                                                            <div class="col-lg-7 col-md-8 col-sm-6 col-xs-12 pdl-sm-7">
                                                                                <div class="form-group">
                                                                                    <input formControlName="permanentMobileNo" maxlength="10" class="form-control" type="text" NumberOnly />
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </tab>

                                                        <tab tabTitle="สถานที่ทำงาน">
                                                            <div class="oneContentTab">
                                                                <div class="boxListForm pdlr0" style="overflow-y: auto;">
                                                                    <div class="col-lg-4 col-md-6">
                                                                        <div class="row">
                                                                            <div class="col-lg-5 col-md-4 col-sm-6 col-xs-12 txt-sm-right pdr-sm-7 lh-form-control ws-nowrap">
                                                                                <font color="red">*</font> สถานที่ทำงาน :
                                                                            </div>
                                                                            <div class="col-lg-7 col-md-8 col-sm-6 col-xs-12 pdl-sm-7">
                                                                                <div class="form-group">
                                                                                    <input class="form-control" id="address" formControlName="workplace" maxlength="32" type="text" NumberCharacterOnly/>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    <div class="col-lg-4 col-md-6">
                                                                        <div class="row">
                                                                            <div class="col-lg-5 col-md-4 col-sm-6 col-xs-12 txt-sm-right pdr-sm-7 lh-form-control ws-nowrap">
                                                                                อาคาร :
                                                                            </div>
                                                                            <div class="col-lg-7 col-md-8 col-sm-6 col-xs-12 pdl-sm-7">
                                                                                <div class="form-group">
                                                                                    <input class="form-control" id="companyVillage" formControlName="companyVillage" maxlength="32" type="text" NumberCharacterOnly />
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    <div class="col-lg-4 col-md-6">
                                                                        <div class="row">
                                                                            <div class="col-lg-5 col-md-4 col-sm-6 col-xs-12 txt-sm-right pdr-sm-7 lh-form-control ws-nowrap">
                                                                                <font color="red">*</font> เลขที่ :
                                                                            </div>
                                                                            <div class="col-lg-7 col-md-8 col-sm-6 col-xs-12 pdl-sm-7">
                                                                                <div class="form-group">
                                                                                    <input class="form-control" maxlength="10" id="address" formControlName="companyAddressNo" type="text" NumberCharacterOnly/>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    <div class="col-lg-4 col-md-6">
                                                                        <div class="row">
                                                                            <div class="col-lg-5 col-md-4 col-sm-6 col-xs-12 txt-sm-right pdr-sm-7 lh-form-control ws-nowrap">หมู่ที่ :</div>
                                                                            <div class="col-lg-7 col-md-8 col-sm-6 col-xs-12 pdl-sm-7">
                                                                                <div class="form-group">
                                                                                    <input class="form-control" maxlength="2" type="text" formControlName="companyMoo" NumberCharacterOnly/>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    <div class="col-lg-4 col-md-6">
                                                                        <div class="row">
                                                                            <div class="col-lg-5 col-md-4 col-sm-6 col-xs-12 txt-sm-right pdr-sm-7 lh-form-control ws-nowrap">ตรอก/ซอย :</div>
