allocation-modal {
    .boxSearchPlanInsurance2 {
        padding-bottom: 25px;
    }
    .boxSearchPlanInsurance2 .boxSelectPlan {
        display: block;
        position: relative;
        padding: 0px 0px 0px 0px !important;
        margin-bottom: 15px;
        // height: 46px;
        // border: 1px solid rgba(255, 255, 255, 0.5);
    }
    .boxStatusStep {
        display: table;
        width: 100%;
        height: 95px;
        min-height: 90px;
        padding: 23px 80px 20px 80px;
        text-align: center;
    }
    .form-control.item-block {
        min-height: auto;
        height: 44px;
        text-align: center;
    }
    .hasBgImg .container {
        position: relative;
        width: 90%;
    }
    .bgPaperShadow .contentPaperShadow2 {
        display: block;
        position: relative;
        min-height: 200px;
        z-index: 1;
        background: #fff;
        padding: 20px;
        color: #333;
        font-size: 25px;
        margin-top: 10px;
    }
    .hasBgImg:before {
        display: block;
        content: '';
        background: #28ABE6 no-repeat center top;
        background-size: 100% 100%;
        position: absolute;
        left: 0px;
        top: 0px;
        width: 100%;
        height: 190px;
    }
    .hasBgImg .container {
        position: relative;
    }
    .boxSearchPlanInsurance2 .label {
        position: relative;
        text-align: center;
        padding-bottom: 20px;
    }
    .boxSearchPlanInsurance2 .label span {
        color: #fff;
        text-align: center;
        font-size: 25px;
    }
    .boxSearchPlanInsurance2 .label {
        position: relative;
        text-align: center;
    }
    @media(max-width:767px) {
        .boxSearchPlanInsurance2 {
            padding-left: 0px;
        }
    }
    @media only screen and (min-width: 768px) and (max-width: 991px) {
        .boxSearchPlanInsurance2 {
            padding-left: 0px;
        }
    }
    @media only screen and (min-width: 1024px) and (max-width: 1200px) {}
    .boxTotalRisk {
        padding-top: 10px;
    }
    .totalRiskText {
        padding: 10px 0px;
        margin: 0px;
        text-align: center;
        color: #1484c6;
        font-size: 50px;
        line-height: 1em;
        font-style: italic;
        font-weight: normal;
    }
    .boxListTabStyle2 {
        height: 40px;
    }
    .boxListTabStyle2 .oneTab {
        height: 40px;
        line-height: 40px;
    }
    .boxListTabStyle2 .oneTab:before {
        display: block;
        content: '';
        width: 1px;
        height: 33px;
        background: #ddd;
        position: absolute;
        left: 0px;
        top: 0px;
    }
    .boxListTabStyle2 .oneTab.action:after {
        height: 8px;
    }
    $grid-columns: 10 !default;
    .riskLevel {
        overflow-x: auto;
    }
    .riskLevel .grid {
        min-width: 840px;
        .row {
            margin-left: 0px;
            margin-right: 0px;
            .col {
                padding: 0px;
                margin-left: 3px;
                text-align: center;
                font-size: 22px;
            }
        }
    }
    .col-risk-01 {
        background-color: #24492E;
        color: white;
        text-align: center;
    }
    .col-risk-02 {
        background-color: #235C37;
        color: white;
        text-align: center;
    }
    .col-risk-03 {
        background-color: #597F34;
        color: white;
        text-align: center;
    }
    .col-risk-04 {
        background-color: #AAA325;
        color: white;
        text-align: center;
    }
    .col-risk-05 {
        background-color: #CF9D2A;
        color: white;
        text-align: center;
    }
    .col-risk-06 {
        background-color: #C46F2C;
        color: white;
        text-align: center;
    }
    .col-risk-07 {
        background-color: #900214;
        color: white;
        text-align: center;
    }
    .col-risk-08 {
        background-color: #A82120;
        color: white;
        text-align: center;
    }
    .rowRisk {
        height: 50px;
        width: 110px;
        line-height: 50px;
        margin-left: 26px;
        //margin-right: 30px;
    }
    .divTable {
        overflow-x: auto;
        padding: 0;
        margin-top: 20px;
        table {
            min-width: 800px;
            width: 100% !important;
            border-bottom: 1px solid #BAD6EB
        }
        table thead,tbody tr {
            display: table;
            width: 100%;
            table-layout: fixed;
        }
        table thead tr th {
            background: #29ABE7;
            color: #fff;
            font-weight: normal;
            padding: 7px 15px;
        }
        table thead tr:last-child th {
            padding-bottom: 5px;
        }
        table thead tr:first-child th {
            padding-top: 5px;
        }
        table tbody tr:nth-child(2n) td {
            background: #F5F5F5;
        }
        table tbody tr td {
            background: #FFFFFF;
            padding: 15px 5px;
        }
        tbody{
            display: block;
            height:460px;
            overflow-y:auto;
            width: 100%;
        }
        thead {
            width: calc( 100% - 1em )
        }
        .th-top{
            background-color: #FFF; 
            color: #63b2e0;
        }
    }
    .fundCode {
        margin: 0px;
        color: rgb(20, 132, 198);
    }
    .fundThName {
        font-size: 20px;
        margin: 0px;
        color: rgb(20, 132, 198);
    }
    .fundInput {
        width: 70px;
        text-align: center;
    }
    .dataCenter {
        text-align: center;
        i {
            color: rgb(20, 132, 198);
            font-size: 35px;
        }
        input {
            border: none;
        }
    }
    .mat-sort-header-arrow {
        margin: 7px 0 0 5px;
    }
    .headrisk {
        width: 200px;
    }
    .headInputRisk {
        width: 165px;
    }
    .hdTitle {
        padding-left: 185px !important;
    }
    .div-btn {
        text-align: center;
        margin-top: 30px;
    }
    .result-title {
        text-align: center;
        height: 65px;
        line-height: 65px;
        background: #29ABE7;
        color: #fff;
        font-size: 35px;
        margin-top: 20px;
    }
    .data-date {
        text-align: right;
        margin-top: 20px;
        margin-right: 10px;
    }
    .tbResult {
        overflow-x: auto;
        table {
            min-width: 800px;
        }
        table tr td {
            border: 1px solid #C8C8C8;
        }
        .fundCode {
            color: #333;
            margin-left: 10px;
            text-align: left;
        }
        .fundThName {
            color: #333;
            margin-left: 10px;
            text-align: left;
        }
        .data-percent {
            text-align: center;
        }
    }
    .header-fund1 {
        background-color: #1685C8;
        color: white;
        text-align: center;
        height: 45px;
        font-size: 30px;
    }
    .stTitle {
        background-color: #b1b1b1;
    }
    .fund-period-head {
        text-align: center;
        height: 45px;
        background-color: #ddd;
    }
    .totalCalFund {
        text-align: center;
        .totalTd {
            background-color: #adcbed;
        }
    }
    .btnCalRisk {
        height: 55px;
        padding-left: 10px;
        padding-right: 5px;
        width: 155px;
        .pieBtn {
            left: 17px;
            top: 10px;
            position: absolute;
            font-size: 37px;
        }
        .txt1 {
            margin-left: 17px;
        }
        .txt2 {
            top: -38px;
            font-size: 16px;
            left: 18px;
        }
        .pdfBtn {
            left: 12px;
            top: 10px;
            position: absolute;
            font-size: 37px;
        }
        .pdftxt {
            margin-left: 17px;
            top: 10px;
        }
    }
    @media only screen and (max-width: 767px) {
        .btnCalRisk {
            .pieBtn {
                top: 9px;
            }
            .txt1 {
                top: -5px;
            }
            .txt2 {
                top: -44px;
            }
            .pdftxt {
                top: 1px;
            }
        }
    }
    // @at-root .memberView {
    //     .modal-wrapper{
    //         left: calc(50% - (850px/2));
    //         top: calc(50% - (500px/2));
    //         position: absolute;
    //         width: 850px;
    //         height: 500px;
    //     }
    // }
    @media only screen and (min-width: 868px) and (min-height: 600px) {
        @at-root .popup-allocation {
            .modal-wrapper{
                left: calc(50% - (850px/2));
                top: calc(50% - (500px/2));
                position: absolute;
                width: 850px;
                height: 500px;
            }
        }
        tbody{
            height: 227px;
        }
    }

    @media only screen and (min-width: 1200px) and (min-height: 600px) {
        @at-root .popup-allocation {
            .modal-wrapper{
                left: calc(50% - (1000px/2));
                top: calc(50% - (600px/2));
                position: absolute;
                width: 1000px;
                height: 600px;
            }
        }
        tbody{
            height: 227px;
        }
    }
    .modal .modal-title {
        border-bottom: none
    }
    
}
