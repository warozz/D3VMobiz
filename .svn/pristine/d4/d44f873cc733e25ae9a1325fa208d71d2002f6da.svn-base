  "calculate": (sum, prospect, utils) => {

    const year = 10; // จ่ายคืน 10 ปี
    const rate_year : number = 10; // เงินคืน 10 % ต่อปี
    const per_rate_year : number = rate_year/100; // แปลงค่า rate เพื่อใช้คำนวณ
    
    const n_sum = utils.formatInt(sum);

    const dividend_peryear :number = Math.round(n_sum * per_rate_year); // เงินคืนต่อปี
    const dividend_all :number = Math.round(n_sum * per_rate_year * year); // เงินคืนต่อปี

    const val0 = Math.round((n_sum * 5) + dividend_peryear); // 5.1 มาจาก 5 เท่าของทุนประกัน + เงินคืนต่อปี
    const val1 = Math.round(n_sum * 1.0); // 1 เท่าของทุนประกัน
    const val2 = Math.round(n_sum * 5.0); // 5 เท่าของทุนประกัน
    const val3 = dividend_all;
    const val4 = Math.round(n_sum * 2.0); // 2 เท่าของทุนประกัน
    const val5 = Math.round(n_sum * 3.0); // 3 เท่าของทุนประกัน
    const val6 = Math.round(n_sum * 4.0); // 4 เท่าของทุนประกัน
    const val7 = dividend_peryear;
    const val8 = Math.round(val2 + val3); // 500% + 100% = 600%


    return {
      data2_1: utils.numFormat(val1),
      data2_2: utils.numFormat(val4),
      data2_3: utils.numFormat(val5),
      data2_4: utils.numFormat(val6),
