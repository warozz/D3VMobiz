  "calculate" : (sum, prospect, utils) => {

    const rate_year : number = 2; // เงินคืน 2% ของทุนประกัน
    const per_rate_year : number = rate_year / 100;
    const max_year_return : number = 8; // จำนวนปีทั้งหมดที่คืนเงิน
    const rate_end_policy : number = 110; // เมื่อครบสัญญารับเงินคืน 100% ของทุนประกัน + 10% ของทุนประกัน
    const per_rate_end_policy : number = rate_end_policy / 100;
    
    const n_sum = utils.formatInt(sum);
    const val1 = Math.round(n_sum * per_rate_year);
    const val2 = Math.round(n_sum * per_rate_year * max_year_return);
    const val3 = Math.round(n_sum * per_rate_end_policy);
    const val4 = Math.round(n_sum * (per_rate_end_policy + per_rate_year * max_year_return));
    const val5 = Math.round(n_sum * per_rate_year * max_year_return);
    const val6 = Math.round(n_sum * per_rate_end_policy);
    const val7 = Math.round(n_sum * (per_rate_end_policy + per_rate_year * max_year_return));

    return {
      data2_1: utils.numFormat(sum),
      data3_1: utils.numFormat(val1),
      data3_2: utils.numFormat(val1),
