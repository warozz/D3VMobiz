<form [formGroup]="appSignPage">
    <div class="boxContentPage bgBlue3 hasBgImg bgImg95">
        <div class="container">
            <div class="bgPaperShadow mgt25">
                <div class="contentPaperShadow">

                    <!-- start boxFixTopJumpMenu -->
                    <div class="boxFixTopJumpMenu">
                        <div id="boxFixJumpMenu" class="boxContentFixJumpMenu">
                            <div class="boxListFixJumpMenu sm-a-mgr0 sm-a-action-fsnormal">
                                <a class="action" (click)="scrollToCenter()">ลงชื่อผู้เอาประกัน</a>
                            </div>
                        </div>
                    </div>
                    <!-- end boxFixTopJumpMenu -->

                    <div id="boxListJumpContent" class="boxListJumpContent">
                        <!-- start oneJumpContent -->
                        <div id="link1" class="oneJumpContent">
                            <h2 class="view-only-xs c-blue boxTitleFixJumpMenu">ลงชื่อผู้เอาประกัน</h2>

                            <div class="boxChkAndInfo">
                                <div class="boxInfoForm">
                                    <div class="col-md-6 pdlr0">
                                        <div class="col-lg-5 col-md-6 col-sm-5 pdr-sm-7 txt-sm-right lh-form-control">
                                            <font color="red"> * </font>ทำที่ :</div>
                                        <div class="col-lg-7 col-md-6 col-sm-7 pdl-sm-7">
                                            <div class="form-group">
                                                <input id="place" formControlName="place" name="" class="form-control" type="text" value="" placeholder="" NumberCharacterOnly maxlength="60" />
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-md-6 pdlr0">
                                        <div class="col-lg-5 col-md-6 col-sm-5 pdr-sm-7 txt-sm-right lh-form-control">วันที่ :</div>
                                        <div class="col-lg-7 col-md-6 col-sm-7 pdl-sm-7">
                                            <div class="form-group hasIcon">
                                                <calendar [id]="submit_date" [(date)]="submitDate"></calendar>
                                                <!-- <calendar [id]="submit_date" [(date)]="appSignPage.value.submitDate" ></calendar>  -->
                                            </div>
                                        </div>
                                    </div>
                                    <div class="clearfix"></div>

                                    <div class="col-md-6 pdlr0">
                                        <div class="col-lg-5 col-md-6 col-sm-5 pdr-sm-7 txt-sm-right lh-form-control">
                                            <font color="red"> * </font>ชื่อพยาน :</div>
                                        <div class="col-lg-7 col-md-6 col-sm-7 pdl-sm-7">
                                            <div class="form-group">
                                                <!-- <input id="witness1_fname" name="" formControlName="witness_fname" class="form-control" type="text" value="" placeholder="" CharacterOnly />-->
                                                <input readonly [info]="fullnameInfo" fullname-popup (fullnameChange)="fullnameChange($event)" id="witness1_fname" formControlName="witness_fname" class="form-control" type="text" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6 pdlr0">
                                        <div class="col-lg-5 col-md-6 col-sm-5 pdr-sm-7 txt-sm-right lh-form-control">
                                            <font color="red"> * </font>ชื่อผู้ขอเอาประกันภัย :</div>
                                        <div class="col-lg-7 col-md-6 col-sm-7 pdl-sm-7">
                                            <div class="form-group">
                                                <!-- <input id="name" formControlName="name" class="form-control" type="text" /> -->
                                                <input readonly [info]="insurefullnameInfo" [birthDate]="birthDate" fullname-popup (fullnameChange)="insureFullnameChange($event)" id="name" formControlName="name" class="form-control" type="text" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="clearfix"></div>
                                    <div class="col-md-6 pdlr0">
                                        <div class="col-lg-5 col-md-6 col-sm-5 pdr-sm-7 txt-sm-right lh-form-control">
                                            <font color="red">*</font> ชื่อพยาน/ผู้เขียน/พิมพ์ :</div>
                                        <div class="col-lg-7 col-md-6 col-sm-7 pdl-sm-7">
                                            <div class="form-group">

                                                <!-- <input id="name" formControlName="name" class="form-control" type="text" value="" placeholder="" /> -->
                                                <input readonly [info]="fullnameInfo2" fullname-popup (fullnameChange)="fullnameChange2($event)" class="form-control" formControlName="witness_fname2" type="text" />
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-md-6 pdlr0">
                                        <div class="col-lg-5 col-md-6 col-sm-5 pdr-sm-7 txt-sm-right lh-form-control">ชื่อตัวแทน :</div>
                                        <div class="col-lg-7 col-md-6 col-sm-7 pdl-sm-7">
                                            <div class="form-group">
                                                <input name="" class="form-control" formControlName="agentName" type="text" value="" placeholder="" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="clearfix"></div>
                                    <div class="col-md-offset-6 col-md-6 pdlr0">
                                        <div class="col-lg-5 col-md-6 col-sm-5 pdr-sm-7 txt-sm-right lh-form-control">ใบอนุญาตเลขที่ :</div>
                                        <div class="col-lg-7 col-md-6 col-sm-7 pdl-sm-7">
                                            <div class="form-group">
                                                <input id="license" name="" class="form-control" formControlName="license" type="text" value="" placeholder="" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="clearfix"></div>
                                </div>
                            </div>

                        </div>
                        <!-- end oneJumpContent -->
                    </div>
                </div>
            </div>
        </div>
    </div>